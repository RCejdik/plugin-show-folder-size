{
    "name": "jfcherng/show-folder-size",
    "description": "A Roundcube plugin which shows folder size.",
    "type": "roundcube-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jfcherng/roundcube-plugin-show-folder-size",
    "version": "0.4.11",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "php": "^5.4.0 || ^7",
        "roundcube/plugin-installer": "~0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "liip/rmt": "^1.5"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix --verbose"
        ]
    }
}
