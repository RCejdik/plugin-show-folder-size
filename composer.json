{
    "name": "jfcherng/show-folder-size",
    "description": "A Roundcube plugin which shows folder size.",
    "type": "roundcube-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jfcherng/roundcube-show-folder-size-plugin",
    "version": "0.2.7",
    "authors": [
        {
            "name": "Jack Cherng",
            "email": "jfcherng@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "php": "^5.4.0 || ^7.0.0",
        "roundcube/plugin-installer": "~0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix --verbose"
        ]
    }
}
